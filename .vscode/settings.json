{
    "editor.formatOnSave": false,
    "javascript.format.enable": false,
    "eslint.alwaysShowStatus": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "eslint.options": {
        "extensions": [
            "*.html",
            "*.js",
            "*.vue",
            "*.jsx",
            "*.json",
            "*.ts"
        ]
    },
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "cSpell.words": [
        "achromatopsia",
        "Combobox",
        "deuteranomaly",
        "idempotency",
        "kubernetes",
        "openapi",
        "pinia",
        "prefecthq",
        "protanomaly",
        "protaponia",
        "pydantic",
        "rrule",
        "semibold",
        "subflow",
        "tailwindcss",
        "tritanomaly",
        "unref",
        "vite",
        "vitejs"
    ],
    "search.exclude": {
        "**/dist": true
    },
    "typescript.tsdk": "node_modules/typescript/lib",
    "files.exclude": {
        "node_modules": true,
        "dist": true
    },
    "css.customData": [
        "./.vscode/css-data.json"
    ],
    "workbench.colorCustomizations": {
        "activityBar.activeBackground": "#e4b849",
        "activityBar.background": "#e4b849",
        "activityBar.foreground": "#15202b",
        "activityBar.inactiveForeground": "#15202b99",
        "activityBarBadge.background": "#158b69",
        "activityBarBadge.foreground": "#e7e7e7",
        "commandCenter.border": "#15202b99",
        "sash.hoverBorder": "#e4b849",
        "statusBar.background": "#daa520",
        "statusBar.foreground": "#15202b",
        "statusBarItem.hoverBackground": "#ad8319",
        "statusBarItem.remoteBackground": "#daa520",
        "statusBarItem.remoteForeground": "#15202b",
        "titleBar.activeBackground": "#daa520",
        "titleBar.activeForeground": "#15202b",
        "titleBar.inactiveBackground": "#daa52099",
        "titleBar.inactiveForeground": "#15202b99"
    },
    "peacock.color": "#DAA520"
}