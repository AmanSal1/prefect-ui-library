        "date-fns-tz": "1.3.0",
        "eslint": "8.6.0",
        "tsc-alias": "1.6.5",
        "typescript": "^4.6.4",
        "vite": "2.4.2",
        "vue-tsc": "0.33.9"
    },
    "peerDependencies": {
        "pinia": "2.0.11",
